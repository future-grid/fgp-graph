{
  "name": "fgp-graph",
  "version": "1.0.0",
  "description": "fgp-graphs is a charting lib and base on \"dygraphs. provides more useful functionalities, such as zoomin & zoomout with scrolling, buttons for download data, export image etc. ",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode development --config webpack.dev.js",
    "build:release": "webpack --mode production --config webpack.prod.js",
    "build:watch": "webpack --progress --colors --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/future-grid/fgp-graph.git"
  },
  "keywords": [
    "fgp",
    "graphs",
    "dygraphs",
    "futuregrid"
  ],
  "author": "eric@future-grid.com.au",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/future-grid/fgp-graph/issues"
  },
  "homepage": "https://github.com/future-grid/fgp-graph#readme",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.42",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.42",
    "@babel/preset-env": "^7.0.0-beta.42",
    "babel-loader": "^8.0.0-beta.2",
    "babel-preset-minify": "^0.5.0",
    "webpack": "^4.35.2",
    "webpack-cli": "^3.3.5"
  },
  "dependencies": {
    "dygraphs": "^2.1.0"
  }
}
